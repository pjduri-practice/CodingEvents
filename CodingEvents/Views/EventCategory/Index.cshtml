@{
}
@using CodingEvents.Models;
@model List<EventCategory>

<h1>All Event Categories</h1>

<table>
    <tr>
    <th>Category Name</th>
    </tr>
    @if (Model != null)
    {
        @foreach (EventCategory category in Model)
        {
            <tr>
                <td>@category.Name</td>
            </tr>
        }

    }
</table>

